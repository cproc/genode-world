<config>
	<parent-provides>
		<service name="CPU"/>
		<service name="LOG"/>
		<service name="Nic"/>
		<service name="Nitpicker"/>
		<service name="PD"/>
		<service name="Report"/>
		<service name="RM"/>
		<service name="ROM"/>
		<service name="Timer"/>
	</parent-provides>

	<default-route> <any-service> <parent/> <any-child/> </any-service> </default-route>

	<start name="falkon" caps="9750">
		<resource name="RAM" quantum="1990M"/>
		<config>
			<vfs>
				<dir name="dev">
					<dir name="pipe"> <pipe/> </dir>
					<log/>
					<jitterentropy name="random"/>
					<jitterentropy name="urandom"/>
					<inline name="rtc">2018-01-01 00:01</inline>
				</dir>
				<dir name="qt">
					<dir name="plugins">
						<dir name="platforms">
							<rom name="libqnitpicker.lib.so"/>
						</dir>
					</dir>
				</dir>
				<dir name="shm"> <shm/> </dir>
				<dir name="socket">
					<lwip dhcp="yes"/>
				</dir>
				<dir name="tmp"> <ram/> </dir>
				<tar name="qt5_dejavusans.tar"/>
				<tar name="qt5_qtquick2.tar"/>
				<tar name="qt5_qtquick2plugin.tar" />
				<tar name="qt5_qtquick_window.tar" />
				<tar name="qt5_windowplugin.tar" />
				<tar name="qt5_qtwebengine.tar" />
				<tar name="qt5_qtwebengineplugin.tar" />
				<tar name="qt5_qtquick_controls.tar" />
				<tar name="qt5_qtquickcontrolsplugin.tar" />
				<ram/>
			</vfs>
			<libc stdout="/dev/log" stderr="/dev/log" pipe="/dev/pipe" rtc="/dev/rtc" socket="/socket" pid="1"/>
		</config>
		<route>
			<service name="ROM" label="egl_drv.lib.so"> <parent label="egl_swrast.lib.so" /> </service>
			<any-service> <parent /> </any-service>
		</route>
	</start>
</config>
